{
    "version": "0.2.0",
    "configurations": [
         {
             "name": ".NET Core Launch (full)",
             "type": "coreclr",
             "request": "launch",
             "preLaunchTask": "Build",
             "program": "${workspaceFolder}/WasaCommunity/bin/Debug/netcoreapp2.1/WasaCommunity.dll",
             "args": [
             ],
             "cwd": "${workspaceFolder}/WasaCommunity",
             "stopAtEntry": false,
             "internalConsoleOptions": "openOnSessionStart",
             "launchBrowser": {
                 "enabled": false
             },
             "env": {
                 "ASPNETCORE_ENVIRONMENT": "Development"
             },
             "sourceFileMap": {
                 "/Views": "${workspaceFolder}/Views"
             },
             "logging": {
                 "moduleLoad": false,
             }
         },
         {
            "name": "Launch Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/WasaCommunity/ClientApp/",
            "breakOnLoad": true,
            "sourceMaps": true,
            "trace": true
          },
     ],
     "compounds": [
         {
             "name": "Full stack",
             "configurations": [".NET Core Launch (full)", "Launch Chrome"]
         }
     ]
   }