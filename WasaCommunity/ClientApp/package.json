{
  "name": "wasacommunity",
  "version": "1.0.0",
  "description": "An application made for Wasa Sweden AB",
  "author": {
    "name": "Isak Vidinghoff",
    "email": "isak.vidinghoff@gmail.com",
    "url": "https://www.isakvidinghoff.com"
  },
  "homepage": "https://www.wasacommunity.com",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"ng serve\" \"npm run theme\"",
    "start:prod": "npm run build-prod && cd dist && angular-http-server --https --key ../sslcert/server.key --cert ../sslcert/server.crt",
    "theme": "node-sass --watch src/app/assets/themes/ --output src/assets/themes/ --output-style=compressed",
    "build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --extract-css",
    "build-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod",
    "test": "ng test",
    "test-single-run": "ng test --watch=false --browsers Chrome_no_sandbox",
    "lint": "ng lint",
    "lint-fix": "ng lint wasacommunity --fix",
    "e2e": "ng e2e",
    "hmr": "concurrently --kill-others \"ng serve --configuration hmr --disable-host-check\" \"npm run theme\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.0",
    "@angular/material": "^7.2.1",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^7.2.0",
    "@angular/service-worker": "^7.2.0",
    "@aspnet/signalr": "^1.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "@ng-bootstrap/schematics": "^2.0.0-alpha.1",
    "@ngx-translate/core": "^11.0.1",
    "@thisissoon/angular-ellipsis": "^1.3.2",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.7.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.2",
    "devextreme": "18.2.8",
    "devextreme-angular": "18.2.8",
    "devextreme-aspnet-data-nojquery": "^2.1.1",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.13.1",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "ng2-charts": "^1.6.0",
    "ngx-perfect-scrollbar": "^7.2.0",
    "ngx-toasta": "^0.1.0",
    "rxjs": "^6.3.3",
    "rxjs-tslint": "^0.1.6",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular/cli": "~7.2.1",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "^5.4.2",
    "puppeteer": "^1.18.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.1",
    "typescript": "~3.2.2",
    "webpack": "^4.28.4"
  }
}
